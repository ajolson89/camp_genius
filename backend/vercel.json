{
  "version": 2,
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/campsites",
      "destination": "/api/campsites"
    }
  ],
  "functions": {
    "api/health.ts": {
      "maxDuration": 30
    },
    "api/campsites.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}